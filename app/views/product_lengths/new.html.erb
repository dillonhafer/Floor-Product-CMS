<h3 class="subheader">New Product Length</h3>
<hr />
<%= form_for @product_length do |f| %>
  
  <%= render partial: 'form', locals: {f: f} %>
  
  <%= f.submit "Create Product Length", class: 'small green nice button radius' %>
  <%= link_to "Cancel", product_lengths_path, class: 'small red nice button radius' %>
<% end %>