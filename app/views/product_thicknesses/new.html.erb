<h3 class="subheader">New Product Thickness</h3>
<hr />
<%= form_for @product_thickness do |f| %>
  
  <%= render partial: 'form', locals: {f: f} %>
  
  <%= f.submit "Create Product Thickness", class: 'small green nice button radius' %>
  <%= link_to "Cancel", product_thicknesses_path, class: 'small red nice button radius' %>
<% end %>