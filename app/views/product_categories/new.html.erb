<h3 class="subheader">New Product Category</h3>
<hr />
<%= form_for @product_category do |f| %>
  
  <%= render partial: 'form', locals: {f: f} %>
  
  <%= f.submit "Create Product Category", class: 'small green nice button radius' %>
  <%= link_to "Cancel", product_categories_path, class: 'small red nice button radius' %>
<% end %>