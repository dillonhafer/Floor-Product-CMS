<h3 class="subheader">Edit Color Family</h3>
<hr />
<%= form_for @color_family do |f| %>

  <%= render partial: 'form', locals: {f: f} %>
  
  <%= f.submit "Update", class: 'small blue nice button radius' %>
  <%= link_to "Cancel", color_families_path, class: 'small red nice button radius' %>
<% end %>