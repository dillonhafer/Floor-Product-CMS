<h3 class="subheader">New Product Width</h3>
<hr />
<%= form_for @product_width do |f| %>
  
  <%= render partial: 'form', locals: {f: f} %>
  
  <%= f.submit "Create Product Width", class: 'small green nice button radius' %>
  <%= link_to "Cancel", product_widths_path, class: 'small red nice button radius' %>
<% end %>